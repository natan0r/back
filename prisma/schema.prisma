// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model UserCliente {
  id             Int      @id @default(autoincrement())
  nomeCompleto   String
  dataNascimento DateTime
  CEP            String
  rua            String
  numero         Int
  bairro         String
  cidade         String
  estado         String
  email          String   @unique
  senha          String
}

model UserEmpresa {
  id          Int    @id @default(autoincrement())
  nomeEmpresa String
  endereco    String
  periodo     String
  horario     String
  tipoServico String
  numero      Int
  email       String @unique
  senha       String
}
